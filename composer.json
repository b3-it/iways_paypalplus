{
    "name": "b3-it/iways_paypalplus",
    "license": "OSL-3.0",
    "version": "1.0",
    "description": "Integrates PayPal Plus into Magento",
    "type": "magento-module",
    "authors": [
        {
            "name": "Leo Friedrichs",
            "email": "lf@kirchbergerknorr.de"
        }
    ],
    "suggest": {
        "magento-hackathon/magento-composer-installer": "^3.0.6"
    },
    "require": {
        "b3-it/paypal-php-sdk": "^1.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/b3-it/PayPal-PHP-SDK.git"
        }
    ],
    "extra": {
        "map": [
            ["app/code/community/Iways/PayPalPlus", "app/code/local/Iways/PayPalPlus"],
            ["app/design/adminhtml/default/default/template/paypalplus", "app/design/adminhtml/default/default/template/paypalplus"],
            ["app/design/frontend/base/default/layout/iways_paypalplus.xml", "app/design/frontend/base/default/layout/iways_paypalplus.xml"],
            ["app/design/frontend/base/default/layout/template/paypalplus", "app/design/frontend/base/default/layout/template/paypalplus"],
            ["app/etc/modules/Iways_PayPalPlus.xml", "app/etc/modules/Iways_PayPalPlus.xml"],
            ["app/locale/de_DE/Iways_PayPalPlus.csv", "app/locale/de_DE/Iways_PayPalPlus.csv"],
            ["skin/frontend/base/default/css/iways-paypalplus.css", "skin/frontend/base/default/css/iways-paypalplus.css"],
            ["skin/frontend/base/default/css/images/iways", "skin/frontend/base/default/css/images/iways"]
        ]
    }
}
